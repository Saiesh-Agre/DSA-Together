vector<int> find(int arr[], int n , int x )
{
    int first=-1, last=-1;
    for(int i=0; i<n;i++){
        if(arr[i] == x){
            first = i;
            break;
        }
    }
    
    for(int j=0; j<n;j++){
        if(arr[j] == x){
            last = j;
        }
    }
    return {first, last};
}
