class Solution:
    
    #Function is to check whether two strings are anagram of each other or not.
    def isAnagram(self,a,b):
        #code here
        s1 = list(a)
        s2 = list(b)
        s1.sort()
        s2.sort()
        if s1 == s2:
            return True
            
        else:
            return False
